@model OrnekCV.Models.entity.tblYeteneklerim
@{
    ViewBag.Title = "YeniYetenek";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h4>Yetenek Ekleme Sayfası</h4>
<br />
<form method="post">
    @Html.Label("Yetenek")
    @Html.TextBoxFor(x => x.Yetenek, new { @class = "form-control" })
    @Html.Label("Oran")
    @Html.TextBoxFor(x=>x.Oran, new {@class="form-control" })
    <br />
    <button class="btn btn-info">Ekle</button>

</form>
