@model  OrnekCV.Models.entity.TblSertifika
@{
    ViewBag.Title = "SertifikaEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Yeni Sertifika Ekle</h2>
<form  method="post">
    @Html.Label("Açıklama")
    @Html.TextBoxFor(x => x.Aciklama, new {@class="form-control"})
    @Html.Label("Tarih")
    @Html.TextBoxFor(x => x.Tarih, new { @class = "form-control" })
    <br />
    <button class="btn btn-info">Ekle</button>

</form>
