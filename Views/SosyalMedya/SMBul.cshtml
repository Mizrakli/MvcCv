@model OrnekCV.Models.entity.TblSosyalMedya
@{
    ViewBag.Title = "SMGetir";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Sosyal Medya Düzenleme Sayfası</h2>
<form method="post">
    @Html.Label("Başlık")
    @Html.TextBoxFor(x => x.Ad, new { @class = "form-control" })
   
    <br />
    @Html.Label("Altbaşlık 1")
    @Html.TextBoxFor(x => x.Link, new { @class = "form-control" })

    <br />
    @Html.Label("Altbaşlık 2")
    @Html.TextBoxFor(x => x.Icon, new { @class = "form-control" })

    <br />
    <button class="btn btn-info">Düzenlemeyi Kaydet</button>
</form>
